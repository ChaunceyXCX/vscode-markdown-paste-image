{
  "name": "vscode-markdown-paste-image",
  "displayName": "Markdown Paste",
  "description": "Automaticaly create file for pasting image content. Parse text content and generator content on your rules for pasting text.",
  "version": "0.6.2",
  "publisher": "telesoho",
  "author": {
    "name": "telesoho",
    "email": "telesoho@gmail.com"
  },
  "license": "(MIT)",
  "repository": {
    "type": "git",
    "url": "https://github.com/telesoho/vscode-markdown-paste-image.git"
  },
  "homepage": "https://github.com/telesoho/vscode-markdown-paste-image",
  "bugs": {
    "url": "https://github.com/telesoho/vscode-markdown-paste-image/issues"
  },
  "icon": "res/icon.png",
  "galleryBanner": {
    "color": "#252526",
    "theme": "dark"
  },
  "keywords": [
    "paste",
    "image",
    "markdown",
    "paste image",
    "paste url"
  ],
  "engines": {
    "vscode": "^1.12.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.MarkdownPaste"
  ],
  "main": "./out/src/extension",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "node ./node_modules/vscode/bin/test"
  },
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Markdown Paste Configuration",
      "properties": {
        "pasteImage.path": {
          "type": "string",
          "default": "./",
          "description": "The destination to save image file."
        },
        "pasteImage.silence": {
          "type": "boolean",
          "default": false,
          "description": "Enable/disable showing confirm box while paste image."
        },
        "MarkdownPaste.rules": {
          "type": "array",
          "default": [{
              "regex": "(?:https?:\/\/)?(?:(?:(?:www\\.?)?youtube\\.com(?:\/(?:(?:watch\\?.*?v=([^&\\s]+).*)|))?))",
              "options": "g",
              "replace": "[![](https://img.youtube.com/vi/$1/0.jpg)](https://www.youtube.com/watch?v=$1)"
            },
            {
              "regex": "(https?:\/\/.*)",
              "options": "ig",
              "replace": "[]($1)"
            }
          ],
          "description": "Rules for markdown paste."
        }
      }
    },
    "commands": [{
      "command": "extension.MarkdownPaste",
      "title": "Markdown Paste"
    }],
    "keybindings": [{
      "command": "extension.MarkdownPaste",
      "key": "ctrl+alt+v",
      "mac": "cmd+alt+v",
      "when": "editorTextFocus"
    }]
  },
  "dependencies": {
    "copy-paste": "^1.2.0",
    "moment": "^2.14.1",
    "shelljs": "^0.7.7"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.32",
    "@types/node": "^6.0.40",
    "mocha": "^2.3.3",
    "shelljs": "^0.7.7",
    "typescript": "^2.0.3",
    "vscode": "^1.1.5"
  }
}